{
  "extends": "../../tsconfig.base.json",
  "compilerOptions": {
    "lib": ["es5", "es6"],
    "strict": false,
    "strictNullChecks": false,
    "alwaysStrict": false,
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "outDir": "./build",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "resolveJsonModule": true,
    "esModuleInterop": true
  }
}
